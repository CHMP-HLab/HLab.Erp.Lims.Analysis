<UserControl x:Class="HLab.Erp.Lims.Analysis.Module.SampleTests.TestView"
                              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                              xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                              xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                              xmlns:local="clr-namespace:HLab.Erp.Lims.Analysis.Module.SampleTests"
                              xmlns:lang="clr-namespace:HLab.Localization.Wpf.Lang;assembly=HLab.Localization.Wpf"
                              xmlns:wpf="clr-namespace:HLab.Icons.Wpf;assembly=HLab.Icons.Wpf"
                              mc:Ignorable="d" 
                              d:DesignHeight="450" d:DesignWidth="800"
            >
    <local:ColumnDescriptionBlock
            Title="{Binding Model.TestName}"
            Description="{Binding Model.Description}"
            IconPath="{Binding Model.TestClass.IconPath}"
    />
</UserControl>
